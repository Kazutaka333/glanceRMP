{
  "name": "RMP",
  "description": "View ratings for instructors on Peralta Passport ",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["rmp-api.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "Rate My Professor on Peralta Passport",
      "default_icons" : { 	"19" : "19icons.png",
		                "38" : "38icons.png"}
  },
    "icons" : {
	                        "16" : "16icons.png",
	                        "48" : "48icons.png",
                        	"128" : "128icons.png"},

  "manifest_version": 2
}
